<%= back_url_hidden_field_tag %>
<%= error_messages_for 'project_list_value' %>

<%= hidden_field_tag :custom_field_id, @project_list_value.custom_field_id %>

<div class="box tabular">
  <p><%= f.text_field :value, :maxlength => 60, :size => 60, :required => true %></p>

  <p><%= f.select :status, ProjectEnumeration::ENUMERATION_STATUSES.collect {|s| [l("version_status_#{s}"), s]} %></p>

  <p><%= f.select :sharing, @project_list_value.allowed_sharings.collect {|v| [format_version_sharing(v), v]} %></p>
</div>
